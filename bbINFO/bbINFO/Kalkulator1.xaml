<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="bbINFO.Kalkulator1">
    <ContentPage.Content>
        <StackLayout>
            <Grid Margin="50" ColumnSpacing="50">
                <Grid.RowDefinitions>
                    <RowDefinition Height="150"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <ImageButton Source="putnicka.png" BackgroundColor="Transparent" BorderColor="Black" CornerRadius="5" BorderWidth="1" Padding="5" Clicked="ImageButton_Clicked"/>
                <ImageButton Grid.Row="0" Grid.Column="1" Source="teretna.png" BackgroundColor="Transparent" BorderColor="Black" CornerRadius="5" BorderWidth="1" Padding="5"/>

            </Grid>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>